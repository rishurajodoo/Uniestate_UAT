<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Inherit Form View to Modify it -->
        <record id="floor_wizard_form_inherit_id" model="ir.ui.view">
            <field name="name">Floor Wizard Form Inherit</field>
            <field name="model">create.floor.wizard</field>
            <field name="inherit_id" ref="ol_property_custom.create_floor_form"/>
            <field name="arch" type="xml">

                <xpath expr="//form" position="inside">
                    <notebook>
                        <page name="floor_wizard" string="Floor Detail">
                            <field name="floor_detail_ids">
                                <tree editable="bottom">
                                    <field name="name"/>
                                    <field name="floor_type_id"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </xpath>

            </field>
        </record>

    </data>
</odoo>