<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="purchase_requisition_seq" model="ir.sequence">
            <field name="name">Purchase Requisition Sequence</field>
            <field name="code">purchase.requisition.seq</field>
            <field name="prefix">PR%(year)s</field>
            <field name="padding">6</field>
        </record>
    </data>

    <data noupdate="1">
        <record id="limit_seq" model="ir.sequence">
            <field name="name">Assign Limit Sequence</field>
            <field name="code">assign.limit.seq</field>
            <field name="prefix">LMT</field>
            <field name="padding">2</field>
        </record>
    </data>

    <data noupdate="1">
        <record id="petty_expense_seq" model="ir.sequence">
            <field name="name">Petty Expenses Sequence</field>
            <field name="code">petty.expense.seq</field>
            <field name="prefix">PC</field>
            <field name="padding">3</field>
        </record>
    </data>
</odoo>


