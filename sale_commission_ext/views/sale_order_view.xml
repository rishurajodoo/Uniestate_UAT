<?xml version="1.0" encoding="utf-8" ?>
<odoo>
   <record id="view_order_form_commission" model="ir.ui.view">
        <field name="name">commission.sale.order.form.view</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='project']" position="attributes">
                <attribute name="context">{'default_for_sale': for_sale, 'default_for_rent': for_rent}</attribute>
            </xpath>
        </field>
    </record>

</odoo>
