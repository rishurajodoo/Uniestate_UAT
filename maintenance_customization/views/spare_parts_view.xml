<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- <data> -->
    <record id="spare_parts_tree_view" model="ir.ui.view">
        <field name="name">spare.parts.tree</field>
        <field name="model">spare.parts</field>
        <field name="arch" type="xml">
            <tree>
                <field name="product_id"/>
                <field name="from_location_id"/>
                <field name="Qty"/>
                <field name="act_Qty"/>
            </tree>
        </field>
    </record>

    <!-- Spare Parts Form View -->
    <record id="spare_parts_form_view" model="ir.ui.view">
        <field name="name">spare.parts.form</field>
        <field name="model">spare.parts</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <!-- Add fields or other elements to the form view -->
                    <field name="product_id"/>
                    <field name="from_location_id" readonly="1"/>
                    <field name="Qty"/>
                    <field name="act_Qty"/>
                    <!-- Add any additional fields as needed -->
                </group>
            </form>
        </field>
    </record>
    <!-- Tree View -->
    <record id="view_checklist_tree" model="ir.ui.view">
        <field name="name">checklist.tree</field>
        <field name="model">check.list</field>
        <field name="arch" type="xml">
            <tree>
                <field name="maintenance_equipment_id"/>

                <field name="description"/>
<!--                <field name="temperature"/>-->
<!--                <field name="pr_bar"/>-->
                <field name="remarks"/>
                <field name="ohter_infor"/>
                <field name="state"/>
            </tree>
        </field>
    </record>

    <!-- Form View -->
    <record id="view_checklist_form" model="ir.ui.view">
        <field name="name">checklist.form</field>
        <field name="model">check.list</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <grou>
                        <field name="maintenance_equipment_id"/>

                        <field name="description"/>
<!--                        <field name="temperature"/>-->
                    </grou>
                    <grou>
<!--                        <field name="pr_bar"/>-->
                        <field name="remarks"/>
                        <field name="ohter_infor"/>
                        <field name="state"/>
                    </grou>


                </group>
            </form>
        </field>
    </record>

