    <record id="maintenance_request_view_form_inherit_mrp_new" model="ir.ui.view">
        <field name="name">maintenance.request.view.form.inherit.hr1</field>
        <field name="model">maintenance.request</field>
        <field name="inherit_id" ref="hr_maintenance.maintenance_request_view_form_inherit_hr"/>
        <field name="arch" type="xml">

            <!--                <xpath expr="//field[@name='employee_id' and not(preceding-sibling::field[@name='employee_id'])]" position="attributes">-->
            <xpath expr="//field[@name='employee_id']" position="attributes">
                <!--                    <attribute name="attrs">{'readonly': [('state', '=', 'Repaired')]}</attribute>-->
                <attribute name="readonly">state == 'Repaired'</attribute>
                <field name="state" invisible="1"/>
            </xpath>

            <!-- For the second occurrence of employee_id -->
            <!--                <xpath expr="//field[@name='employee_id' and preceding-sibling::field[@name='employee_id']]" position="attributes">-->
            <xpath expr="//field[@name='employee_id']" position="attributes">

                <!--                    <attribute name="attrs">{'readonly': [('state', '=', 'Repaired')]}</attribute>-->
                <attribute name="readonly">state == 'Repaired'</attribute>
                <field name="state" invisible="1"/>
            </xpath>
            <xpath expr="//field[@name='employee_id']" position="after">
                <!-- <attribute name="attrs">{'readonly': [('stage_id', 'in', [2])]}</attribute> -->
                <field name="state" invisible="1"/>
            </xpath>

        </field>
    </record>
