    <record id="tenant_view_partner_form" model="ir.ui.view">
        <field name="name">res.partner.tenant.form.inherit</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='category_id']" position="after">
                <field name="tenant_id" invisible="company_type != 'company'"/>
            </xpath>
        </field>
    </record>

    <record id="product_view_inherit_form" model="ir.ui.view">
        <field name="name">product.form.inherit</field>
        <field name="model">product.product</field>
        <field name="inherit_id" ref="product.product_normal_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='is_parking']" position="after">
                <field name="parking_space"/>
            </xpath>
        </field>
    </record>

